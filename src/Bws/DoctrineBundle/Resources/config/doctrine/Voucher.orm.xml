<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Bws\DoctrineBundle\Entity\Voucher" table="voucher">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="code" type="string" column="code" length="45" nullable="true"/>
    <field name="reductionMonetary" type="float" column="reduction_monetary" precision="10" scale="0" nullable="true"/>
    <field name="reductionPercent" type="float" column="reduction_percent" precision="10" scale="0" nullable="true"/>
  </entity>
</doctrine-mapping>
